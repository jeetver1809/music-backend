{
  "name": "music-jam-server",
  "version": "1.0.0",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "restart:pm2": "pm2 reload ecosystem.config.js --env production",
    "stop:pm2": "pm2 stop ecosystem.config.js",
    "logs:pm2": "pm2 logs --lines 200",
    "lint": "eslint . || true"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pm2": "^5.3.0",
    "socket.io": "^4.7.2",
    "ytsr": "^3.8.0",
    "youtubei.js": "^5.8.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "eslint": "^8.0.0"
  }
}
